{
  "scripts": {
    "format": "prettier --write \"./src/**/*.{html,vue,ts,js,json,md,cjs,scss}\"",
    "format:-------des": "格式化代码",
    "lint": "npm run format && npm run lint:script && npm run lint:style",
    "lint:-------des": "格式化代码并校验",
    "lint:script": "eslint --ext .js,.jsx,.cjs,.ts,.tsx,.vue --fix --quiet ./",
    "lint:script:-------des": "格式化script代码",
